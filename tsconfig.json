{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "diagnostics": true,
    "downlevelIteration": false,
    "lib": ["es5", "es6", "dom"],
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "pretty": true,
    "skipLibCheck": true,
    "target": "es5"
  },
  "include": ["src"],
  "exclude": ["dist", "node_modules", "test/typescript"]
}
